@{
    ViewData["Title"] = "Dashboard";
}

<head>
    <!-- Vínculo correcto al archivo CSS -->
    <link rel="stylesheet" href="/css/clubsmart.css" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons/font/bootstrap-icons.css">
</head>

<div class="clubsmart-page">
    <div class="clubsmart-container">
        <!-- Contenido principal -->
        <div class="clubsmart-card">
            <h2 class="clubsmart-header">Bienvenido, @ViewData["UsuarioNombre"]</h2>
            <p class="clubsmart-paragraph">
                Esta es la página principal protegida, accesible solo si estás logueado.
            </p>
        </div>
    </div>
</div>
